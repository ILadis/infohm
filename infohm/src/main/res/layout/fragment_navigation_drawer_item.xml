<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	style="?attr/drawerItemStyle">

	<ImageView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_centerVertical="true"
		android:layout_marginLeft="?attr/horizontalSpaceBorder"
		android:src="@drawable/drawer_item_newsfeed" />

	<TextView
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_centerVertical="true"
		android:layout_marginLeft="?attr/horizontalSpaceKeyline"
		android:text="@string/fragment_navigation_drawer_newsfeed" />

</RelativeLayout>