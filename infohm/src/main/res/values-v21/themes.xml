<?xml version="1.0" encoding="utf-8"?>
<resources>

	<!--
		Base application theme for API 21+. This theme completely replaces
		AppBaseTheme from BOTH res/values/styles.xml, res/values-v11/styles.xml
		and res/values-v14/styles.xml on API 21+ devices.
	-->
	<style name="AppBaseTheme" parent="android:Theme.Material">
		<!-- API 21 theme customizations can go here. -->
	</style>
	
	<!--
		Base dialog theme for API 21+. This theme completely replaces
		DialogBaseTheme from BOTH res/values/styles.xml, res/values-v11/styles.xml
		and res/values-v14/styles.xml on API 21+ devices.
	-->
	<style name="DialogBaseTheme" parent="android:Theme.Material">
		<!-- API 21 theme customizations can go here. -->
		<item name="android:windowActionBar">false</item>
		<item name="android:windowActionModeOverlay">true</item>
		<item name="android:windowCloseOnTouchOutside">true</item>
	</style>

</resources>
